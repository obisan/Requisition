<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dubinets.requisition">

    <application
        android:name=".activity.DaoApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.order.OrderDetailedActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.order.ClientListActivity"
            android:label="@string/app_name"/>

    </application>

    <meta-data
        android:name="droidparts_dependency_provider"
        android:value=".DependencyProvider" />

</manifest>