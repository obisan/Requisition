<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dubinets.requisition">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:name=".databasehelper.DaoApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".activity.ItineraryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.ClientActivity"
            android:label="@string/app_name"/>

        <activity
            android:name=".activity.SpotActivity"/>

        <activity
            android:name=".activity.ItemActivity"/>

        <activity
            android:name=".activity.ClientDialog"/>

        <activity
            android:name=".activity.navigation.ClientActivity"/>

        <activity
            android:name=".activity.navigation.ItemActivity"/>

        <activity
            android:name=".activity.navigation.TypeActivity"/>

        <activity
            android:name=".activity.dialog.ClientDialog"/>

        <activity
            android:name=".activity.dialog.ItemDialog"/>

        <activity
            android:name=".activity.dialog.TypeDialog"/>

        <activity
            android:name=".activity.info.ClientInfo"/>

        <activity
            android:name=".activity.info.ItemInfo"/>

        <activity
            android:name=".activity.info.TypeInfo"/>

        <activity
            android:name=".activity.SettingsActivity"/>

    </application>

    <meta-data
        android:name="droidparts_dependency_provider"
        android:value=".DependencyProvider" />

</manifest>